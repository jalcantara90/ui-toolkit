{
  "cli": {
    "analytics": "41cb567a-0fa8-40c1-bd7b-9ac5931284fb"
  },
  "version": 1,
  "projects": {
    "sandbox": {
      "projectType": "application",
      "root": "apps/sandbox",
      "sourceRoot": "apps/sandbox/src",
      "prefix": "sysmian-toolkit",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sandbox",
            "index": "apps/sandbox/src/index.html",
            "main": "apps/sandbox/src/main.ts",
            "polyfills": "apps/sandbox/src/polyfills.ts",
            "tsConfig": "apps/sandbox/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/sandbox/src/favicon.ico",
              "apps/sandbox/src/assets"
            ],
            "styles": ["apps/sandbox/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/sandbox/src/environments/environment.ts",
                  "with": "apps/sandbox/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sandbox:build:production"
            },
            "development": {
              "browserTarget": "sandbox:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sandbox:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sandbox/src/**/*.ts",
              "apps/sandbox/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sandbox"],
          "options": {
            "jestConfig": "apps/sandbox/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "sandbox-e2e": {
      "root": "apps/sandbox-e2e",
      "sourceRoot": "apps/sandbox-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sandbox-e2e/cypress.json",
            "devServerTarget": "sandbox:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sandbox:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sandbox-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["sandbox"]
    },
    "ui-toast": {
      "projectType": "library",
      "root": "libs/ui/toast",
      "sourceRoot": "libs/ui/toast/src",
      "prefix": "sysmian",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/ui/toast"],
          "options": {
            "project": "libs/ui/toast/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ui/toast/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/ui/toast/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/toast"],
          "options": {
            "jestConfig": "libs/ui/toast/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/toast/src/**/*.ts",
              "libs/ui/toast/src/**/*.html"
            ]
          }
        }
      },
      "tags": ["type:ui"]
    }
  }
}
